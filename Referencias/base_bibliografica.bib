journal = {Lab Chip},
keywords = {Cell Membrane,Cell Membrane: chemistry,Filtration,Filtration: instrumentation,Filtration: trends,Microfluidic Analytical Techniques,Microfluidic Analytical Techniques: methods,Motion,Nanostructures,Nanostructures: chemistry,Static Electricity},
number = {1},
pages = {23--33},
pmid = {18094759},
title = {{Molecular sieving using nanofilters: past, present and future.}},
url = {http://pubs.rsc.org/en/Content/ArticleHTML/2008/LC/B714128A http://xlink.rsc.org/?DOI=B714128A},
