keywords = {Cargo,Degradation,Mesoporous,Morphology,Nanoparticle,Silica},
pages = {1},
title = {{Characterization and Comparison of Mesoporous Silica Particles for Optimized Drug Delivery}},
url = {http://www.intechopen.com/journals/nanomaterials_and_nanotechnology/characterization-and-comparison-of-mesoporous-silica-particles-for-optimized-drug-delivery},
year = {2014}
}
