file = {:home/gustavo/Documentos/Mendeley Desktop/10.1.1.511.35.pdf:pdf},
isbn = {1387-1811},
issn = {13871811},
journal = {Microporous Mesoporous Mater.},
keywords = {Adsorption,Alkaline treatment,Isotherm,Mesoporous materials,Microporous materials,Pore size distribution,Pore size model,Post-synthesis treatment,Steam treatment,Zeolites},
