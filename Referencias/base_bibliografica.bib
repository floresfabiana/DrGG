issn = {00032700},
journal = {Anal. Chem.},
number = {1},
pages = {178--184},
pmid = {15623294},
title = {{Si-supported mesoporous and microporous oxide interconnects as electrophoretic gates for application in microfluidic devices}},
volume = {77},
year = {2005}
}
